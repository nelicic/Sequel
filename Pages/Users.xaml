<Page x:Class="WPFUIKitProfessional.Pages.Users"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WPFUIKitProfessional.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Users" FontFamily="Poppins">

    <Grid Background="{DynamicResource PrimaryBackgroundColor}">
        <StackPanel>
            <TextBlock Name="id" Height="20" Margin="10" Width="300" Background="{DynamicResource SecundaryBackgroundColor}"></TextBlock>
            <TextBlock Name="login" Height="20" Margin="10" Width="300" Background="{DynamicResource SecundaryBackgroundColor}"></TextBlock>
            <TextBlock Name="password" Height="20" Margin="10" Width="300" Background="{DynamicResource SecundaryBackgroundColor}"></TextBlock>
        </StackPanel>
    </Grid>
</Page>
