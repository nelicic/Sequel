<Window x:Class="WPFUIKitProfessional.Authorization.Account"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFUIKitProfessional.Authorization"
        mc:Ignorable="d"
        Title="Login" Height="550" Width="400" 
        ResizeMode="NoResize" WindowStartupLocation="CenterScreen" FontFamily="Poppins">
    <Grid>
        <Border Grid.Row="1" Background="Transparent">
            <Frame x:Name="authorizationFrameContent" NavigationUIVisibility="Hidden"/>
        </Border>
    </Grid>
</Window>
